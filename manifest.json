{
  "manifest_version": 3,
  "name": "Find",
  "version": "1.0",
  "description": "A search bar to find what you want in a website",
  "permissions": [
    "activeTab",
    "tabs",
    "storage",
    "cookies",
    "scripting"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "Find"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["seedrandom.min.js", "content.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["settings.html", "settings.css", "settings.js"],
      "matches": ["<all_urls>"]
    }
  ]
}