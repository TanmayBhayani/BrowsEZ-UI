{
  "manifest_version": 3,
  "name": "BrowsEZ",
  "version": "1.0",
  "description": "Browser Assistant",
  "permissions": [
    "storage",
    "activeTab",
    "tabs",
    "scripting",
    "cookies",
    "sidePanel"
  ],
  "host_permissions": [
    "https://find-production.up.railway.app/*",
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "action": {
    "default_title": "Open BrowsEZ sidebar"
  },
  "side_panel": {
    "default_path": "sidebar_ui/dist/index.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["seedrandom.min.js", "dist/content-entry.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["dist/content-entry.js"],
      "matches": ["<all_urls>"]
    }
  ]
}